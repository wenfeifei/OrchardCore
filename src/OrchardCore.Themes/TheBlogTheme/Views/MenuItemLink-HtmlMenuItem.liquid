{% assign link = Model.ContentItem.Content.HtmlMenuItemPart %}

{% if Model.HasItems %}
    <a href="{{ link.Url | href }}" class="nav-link dropdown-toggle" data-toggle="dropdown">{{ link.Html | raw }}<b class="caret"></b></a>
{% else %}
    <a class="nav-link" href="{{ link.Url | href }}">{{ link.Html | raw }}</a>
{% endif %}